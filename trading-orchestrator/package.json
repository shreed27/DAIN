{
    "name": "trading-orchestrator",
    "version": "0.1.0",
    "description": "Autonomous trading agent orchestration layer",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "start:server": "tsx src/server.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:integration": "jest --testPathPattern=integration",
        "test:sidex": "jest --testPathPattern='(Sidex|Polymarket|Strategy|server)'",
        "lint": "eslint src --ext .ts",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "trading",
        "autonomous-agents",
        "solana",
        "defi"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@anthropic-ai/sdk": "^0.74.0",
        "@solana/web3.js": "^1.87.6",
        "axios": "^1.6.5",
        "base64id": "^2.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.1",
        "express": "^4.18.2",
        "socket.io": "^4.6.1",
        "viem": "^2.7.1",
        "ws": "^8.16.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.5",
        "@types/supertest": "^6.0.2",
        "@types/ws": "^8.5.10",
        "@typescript-eslint/eslint-plugin": "^6.19.1",
        "@typescript-eslint/parser": "^6.19.1",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.2",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    }
}
